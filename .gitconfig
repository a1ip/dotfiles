[user]
	name = Phil Rigovanov
	email = phil@rigovanov.ru
[push]
	default = simple
[core]
	excludesfile = /Users/phil/.gitignore_global
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[init]
	templatedir = /Users/phil/.git-templates
